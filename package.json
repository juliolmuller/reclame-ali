    "popper.js": "^1.16.1",
    "toastr": "2.*"
  },
  "devDependencies": {
    "cross-env": "6.*",
    "laravel-mix": "5.*",
    "lodash": "4.*",
    "resolve-url-loader": "3.*",
    "sass": "^1.25.0",
